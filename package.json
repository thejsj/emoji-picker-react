{
  "version": "4.15.0",
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://ealush.com/emoji-picker-react",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "description": "Emoji Picker component for React Applications on the web",
  "keywords": [
    "emoji-picker",
    "react-emoji-picker",
    "emoji",
    "emojis",
    "reactions",
    "reactions-picker",
    "emoji-reactions"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ealush/emoji-picker-react.git"
  },
  "scripts": {
    "start": "node ./scripts/build.js --watch",
    "build": "node ./scripts/prepare.js && pnpm type-check && node ./scripts/build.js && node ./scripts/copyData.js",
    "type-check": "tsc --noEmit --incremental --pretty --project tsconfig.json",
    "test": "tsdx test --passWithNoTests",
    "lint": "eslint ./src --ext .ts,.tsx",
    "prepare": "pnpm type-check && node ./scripts/build.js",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "dev": "cd example && npm start"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true
  },
  "name": "emoji-picker-react",
  "author": "ealush",
  "module": "dist/emoji-picker-react.esm.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/emoji-picker-react.esm.js",
      "require": "./dist/index.js"
    }
  },
  "size-limit": [
    {
      "path": "dist/emoji-picker-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/emoji-picker-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "@storybook/addon-essentials": "8.1.1",
    "@storybook/addon-links": "8.1.1",
    "@storybook/addon-webpack5-compiler-swc": "1.0.2",
    "@storybook/react": "8.1.1",
    "@storybook/react-webpack5": "8.1.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "emoji-datasource": "^15.1.2",
    "esbuild": "^0.20.2",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.0.1",
    "storybook": "8.1.1",
    "tiny-invariant": "^1.2.0",
    "tslib": "^2.4.0",
    "tsup": "^8.0.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "flairup": "1.0.0"
  }
}
